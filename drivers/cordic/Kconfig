menuconfig CORDIC
	bool "CORDIC drivers"

if CORDIC

config CORDIC_INIT_PRIORITY
	int "CORDIC devices init priority"
	default 90
	help
	  CORDIC devices initialization priority.

module = CORDIC
module-str = CORDIC
source "subsys/logging/Kconfig.template.log_config"

source "drivers/CORDIC/Kconfig.stm32"

endif # CORDIC
